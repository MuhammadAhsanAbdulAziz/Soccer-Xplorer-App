<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.soccerxplorer.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register"/>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.soccerxplorer.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_favouriteFragment"
            app:destination="@id/favouriteFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_fixtureFragment"
            app:destination="@id/fixtureFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.soccerxplorer.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/welcomeFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment" />
        <action
            android:id="@+id/action_loginFragment_to_adminHomeFragment"
            app:destination="@id/adminHomeFragment"
            app:popUpTo="@id/welcomeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/favouriteFragment"
        android:name="com.example.soccerxplorer.FavouriteFragment"
        android:label="fragment_favourite"
        tools:layout="@layout/fragment_favourite" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.soccerxplorer.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/createPlayerFragment"
        android:name="com.example.soccerxplorer.view.admin.CreatePlayerFragment"
        android:label="fragment_create_player"
        tools:layout="@layout/fragment_create_player" />
    <fragment
        android:id="@+id/createTeamFragment"
        android:name="com.example.soccerxplorer.view.admin.CreateTeamFragment"
        android:label="fragment_create_team"
        tools:layout="@layout/fragment_create_team" />
    <fragment
        android:id="@+id/createFeedbackFragment"
        android:name="com.example.soccerxplorer.view.admin.CreateFeedbackFragment"
        android:label="fragment_create_feedback"
        tools:layout="@layout/fragment_create_feedback" />
    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.example.soccerxplorer.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" />
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.soccerxplorer.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/welcomeFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_welcomeFragment_to_adminHomeFragment"
            app:destination="@id/adminHomeFragment"
            app:popUpTo="@id/welcomeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/createLeagueFragment"
        android:name="com.example.soccerxplorer.view.admin.CreateLeagueFragment"
        android:label="fragment_create_league"
        tools:layout="@layout/fragment_create_league" />
    <fragment
        android:id="@+id/createFixtureFragment"
        android:name="com.example.soccerxplorer.view.admin.CreateFixtureFragment"
        android:label="fragment_create_fixture"
        tools:layout="@layout/fragment_create_fixture" />
    <fragment
        android:id="@+id/adminHomeFragment"
        android:name="com.example.soccerxplorer.view.admin.AdminHomeFragment"
        android:label="fragment_admin_home"
        tools:layout="@layout/fragment_admin_home" >
        <action
            android:id="@+id/action_adminHomeFragment_to_createFeedbackFragment"
            app:destination="@id/createFeedbackFragment" />
        <action
            android:id="@+id/action_adminHomeFragment_to_allPlayerFragment"
            app:destination="@id/allPlayerFragment" />
        <action
            android:id="@+id/action_adminHomeFragment_to_allLeagueFragment"
            app:destination="@id/allLeagueFragment" />
        <action
            android:id="@+id/action_adminHomeFragment_to_allFixtureFragment"
            app:destination="@id/allFixtureFragment" />
        <action
            android:id="@+id/action_adminHomeFragment_to_allTeamFragment"
            app:destination="@id/allTeamFragment" />
    </fragment>
    <fragment
        android:id="@+id/allPlayerFragment"
        android:name="com.example.soccerxplorer.view.admin.AllPlayerFragment"
        android:label="fragment_all_player"
        tools:layout="@layout/fragment_all_player" >
        <action
            android:id="@+id/action_allPlayerFragment_to_createPlayerFragment"
            app:destination="@id/createPlayerFragment" />
    </fragment>
    <fragment
        android:id="@+id/allFixtureFragment"
        android:name="com.example.soccerxplorer.view.admin.AllFixtureFragment"
        android:label="fragment_all_fixture"
        tools:layout="@layout/fragment_all_fixture" >
        <action
            android:id="@+id/action_allFixtureFragment_to_createFixtureFragment"
            app:destination="@id/createFixtureFragment" />
    </fragment>
    <fragment
        android:id="@+id/allTeamFragment"
        android:name="com.example.soccerxplorer.view.admin.AllTeamFragment"
        android:label="fragment_all_team"
        tools:layout="@layout/fragment_all_team" >
        <action
            android:id="@+id/action_allTeamFragment_to_createTeamFragment"
            app:destination="@id/createTeamFragment" />
    </fragment>
    <fragment
        android:id="@+id/allLeagueFragment"
        android:name="com.example.soccerxplorer.view.admin.AllLeagueFragment"
        android:label="fragment_all_league"
        tools:layout="@layout/fragment_all_league" >
        <action
            android:id="@+id/action_allLeagueFragment_to_createLeagueFragment"
            app:destination="@id/createLeagueFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.soccerxplorer.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/fixtureFragment"
        android:name="com.example.soccerxplorer.FixtureFragment"
        android:label="fragment_fixture"
        tools:layout="@layout/fragment_fixture" >
        <action
            android:id="@+id/action_fixtureFragment_to_fixtureDetailFragment"
            app:destination="@id/fixtureDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/fixtureDetailFragment"
        android:name="com.example.soccerxplorer.FixtureDetailFragment"
        android:label="fragment_fixture_detail"
        tools:layout="@layout/fragment_fixture_detail" />
</navigation>